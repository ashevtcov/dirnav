{
  "name": "dirnav",
  "version": "0.0.9",
  "description": "Directional navigation library for JavaScript applications to support 10 foot (TV) experience",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "mocha --require @babel/register --require .mocha.init tests/*.js",
    "coverage": "nyc --reporter=html npm test",
    "flow": "flow",
    "lint": "eslint ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashevtcov/dirnav.git"
  },
  "keywords": [
    "directional",
    "navigation",
    "10foot",
    "tv"
  ],
  "author": "Andrei Shevtcov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashevtcov/dirnav/issues"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "tests/**",
      "build"
    ]
  },
  "homepage": "https://github.com/ashevtcov/dirnav#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.93.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0"
  }
}
